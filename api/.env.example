# Environment: 'dev' (uses proxy) or 'prod' (direct connection)
ENVIRONMENT=dev

# ScyllaDB Connection Settings (for production/direct connection)
SCYLLA_HOST=localhost
SCYLLA_PORT=9042
SCYLLA_CONNECT_TIMEOUT=10.0

# Optional: Override proxy defaults
# SCYLLA_PROXY_DEFAULT_HOST=64.71.146.81
# SCYLLA_PROXY_DEFAULT_PORT=9037

# Optional: Custom proxy mapping (JSON format)
# SCYLLA_PROXY_MAPPING={"172.27.0.2": ["64.71.146.81", 9037], "172.27.0.3": ["64.71.146.81", 9038]}
